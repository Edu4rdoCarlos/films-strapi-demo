{
  "kind": "collectionType",
  "collectionName": "films",
  "info": {
    "singularName": "film",
    "pluralName": "films",
    "displayName": "film"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "released": {
      "type": "date"
    },
    "director": {
      "type": "string"
    },
    "plot": {
      "type": "text"
    },
    "slug": {
      "type": "text"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "Film"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "films"
    }
  }
}
